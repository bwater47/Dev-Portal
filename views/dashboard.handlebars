<section>
    <div class="container">
        <div class="row">
        <div class="col-md-12">
            <h1>Dashboard</h1>
        </div>
        </div>
    </div>
</section>

{{#if loggedIn}}
    <section>
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <h2>Posts</h2>
                    <ul>
                        {{#each postArray}}
                            <li>
                                <h3>{{title}}</h3>
                                <p>{{post_content}}</p>
                                <a href="/editPost/{{id}}">Edit Post</a>
                            </li>
                        {{/each}}
                    </ul>
                </div>
            </div>
        </div>
    </section>
{{/if}}

